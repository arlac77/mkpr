{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/mkpr.mjs",
  "description": "create pull request by pipeing repository content through filter like sed",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "bin/mkpr"
  },
  "scripts": {
    "prepare": "rollup -c",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/mkpr.mjs --section=API",
    "lint": "documentation lint src/mkpr.mjs"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.43",
    "bitbucket-repository-provider": "^3.5.0",
    "commander": "^5.0.0",
    "content-entry": "^2.3.1",
    "execa": "^4.0.0",
    "fast-json-patch": "^3.0.0-1",
    "github-repository-provider": "^7.7.4",
    "local-repository-provider": "^6.5.12",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "ava": "^3.6.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.2.0",
    "rollup": "^2.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-consts": "^1.0.1",
    "rollup-plugin-executable": "^1.5.3",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
