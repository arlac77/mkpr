{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/mkpr-cli.mjs",
  "exports": {
    ".": "./src/mkpr-cli.mjs",
    "./": "./"
  },
  "description": "create pull request by pipeing repository content through filter like sed",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "src/mkpr-cli.mjs"
  },
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/mkpr-cli.mjs --section=API",
    "lint": "documentation lint ./src/mkpr-cli.mjs"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.59",
    "bitbucket-repository-provider": "^3.5.16",
    "commander": "^5.1.0",
    "content-entry": "^2.3.4",
    "execa": "^4.0.0",
    "fast-json-patch": "^3.0.0-1",
    "github-repository-provider": "^7.9.8",
    "local-repository-provider": "^6.5.29",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "c8": "^7.1.1",
    "documentation": "^12.3.0",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
