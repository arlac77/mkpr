{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/mkpr-cli.mjs",
  "module": "src/index.mjs",
  "svelte": "src/index.svelte",
  "description": "create pull request by pipeing repository content through filter like sed",
  "keywords": [
    "svelte"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "src/mkpr-cli.mjs"
  },
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 500 --app \"rollup -c example/rollup.config.mjs -w\"",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/mkpr-cli.mjs --section=API",
    "lint": "documentation lint src/mkpr-cli.mjs"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.53",
    "bitbucket-repository-provider": "^3.5.9",
    "commander": "^5.1.0",
    "content-entry": "^2.3.2",
    "execa": "^4.0.0",
    "fast-json-patch": "^3.0.0-1",
    "github-repository-provider": "^7.9.1",
    "local-repository-provider": "^6.5.21",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "ava": "^3.7.1",
    "c8": "^7.1.0",
    "documentation": "^12.3.0",
    "rollup": "^2.7.2",
    "semantic-release": "^17.0.7",
    "svelte": "^3.21.0",
    "testcafe": "^1.8.4"
  },
  "engines": {
    "node": ">=13.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only",
      "arlac77/template-svelte-component"
    ]
  }
}
