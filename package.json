{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/mkpr.mjs",
  "description": "create pull request by modifying repo file content through a filter (stdin/stdout)",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "bin/mkpr"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/mkpr.mjs --section=API",
    "lint": "documentation lint src/mkpr.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.3.5",
    "commander": "^2.20.0",
    "content-entry": "^2.1.2",
    "execa": "^1.0.0",
    "fast-json-patch": "^2.1.0",
    "github-repository-provider": "^7.1.0",
    "local-repository-provider": "^6.2.5",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^4.1.4",
    "documentation": "^10.1.0",
    "esm": "^3.2.22",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.11.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-executable": "^1.4.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
