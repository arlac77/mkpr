{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/mkpr.mjs",
  "description": "create pull request by pipeing repository content through filter like sed",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "bin/mkpr"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/mkpr.mjs --section=API",
    "lint": "documentation lint src/mkpr.mjs",
    "prepare": "rollup -c",
    "test": "ava --timeout 2m"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.41",
    "bitbucket-repository-provider": "^3.4.2",
    "commander": "^4.1.1",
    "content-entry": "^2.2.6",
    "execa": "^4.0.0",
    "fast-json-patch": "^3.0.0-1",
    "github-repository-provider": "^7.6.2",
    "local-repository-provider": "^6.5.6",
    "semver": "^7.1.2"
  },
  "devDependencies": {
    "ava": "^3.3.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "rollup": "^1.31.0",
    "semantic-release": "^17.0.2"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
