{
  "name": "mkpr",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/mkpr.mjs",
  "description": "create pull request by pipeing repository content through filter like sed",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "mkpr": "bin/mkpr"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/mkpr.mjs --section=API",
    "lint": "documentation lint src/mkpr.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.6.36",
    "bitbucket-repository-provider": "^3.3.41",
    "commander": "^4.0.0",
    "content-entry": "^2.2.4",
    "execa": "^4.0.0",
    "fast-json-patch": "^3.0.0-1",
    "github-repository-provider": "^7.5.1",
    "local-repository-provider": "^6.4.54",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.0.0",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.27.13",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^15.14.0"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mkpr.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mkpr/issues"
  },
  "homepage": "https://github.com/arlac77/mkpr#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
